{
  "manifest_version": 3,
  "name": "Samuray New Tab",
  "version": "1.0",
  "description": "A custom new tab page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "privacy", "windows"],
  "browser_specific_settings": {
    "gecko": {
      "id": "samuray-new-tab@example.com",
      "strict_min_version": "109.0"
    }
  }
}
